{
    "name": "What's run",
    "description": "what is run in this website?",
    "version": "0.0.01",
    "manifest_version": 2,
    "author": "github.com/Attax",
    "homepage_url": "https://www.whatsrun.com/?source=plugin",
    "icons": {
        "16": "icon/favicon-16x16.png",
        "32": "icon/favicon-32x32.png",
        "48": "icon/android-icon-48x48.png",
        "128": "icon/apple-icon-120x120.png"
    },
    "browser_action": {
        "default_title": "What's run",
        "default_icon": "icon/apple-icon-60x60.png"
    },
    "default_locale": "en",
    "background": {
        "page": "background.html"
    },
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
    "content_scripts": [{
        "js": ["static/js/vue.min.js", "static/js/axios.min.js", "static/js/injector.js"],
        "css": ["static/css/common.css"],
        "matches": ["http://*/*", "https://*/*"],
        "run_at": "document_start"
    }],
    "permissions": [
        "cookies",
        "http://*/*",
        "https://*/*",
        "management",
        "tabs",
        "contextMenus",
        "history",
        "webRequest",
        "webNavigation",
        "storage",
        "notifications"
    ]
}